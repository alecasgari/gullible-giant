{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "src/*"
      ],
      "tina-client": [
        ".tina/__generated__/client",
        "tina/__generated__/client"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}